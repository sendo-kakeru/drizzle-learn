{
	"name": "@repo/shared",
	"version": "1.0.0",
	"type": "module",
	"exports": {
		"./*": {
			"types": "./dist/*.d.ts",
			"import": "./dist/esm/*.js",
			"require": "./dist/*.js"
		}
	},
	"scripts": {
		"dev": "pnpm run \"/^dev:/\"",
		"dev:tsup": "tsup --watch",
		"dev:tsc": "tsc --watch",
		"build": "pnpm run build:tsup && pnpm run build:tsc",
		"build:tsup": "tsup --clean",
		"build:tsc": "tsc",
		"lint": "biome check .",
		"format": "biome check --fix --unsafe .",
		"type-check": "tsc --noEmit"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"valibot": "^1.1.0"
	},
	"devDependencies": {
		"@repo/typescript-config": "workspace:*",
		"@repo/tsup-config": "workspace:*"
	}
}
